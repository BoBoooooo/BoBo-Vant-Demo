{
  "name": "anshare-vant-demo",
  "version": "0.1.0",
  "private": true,
  "description": "基础demo框架",
  "author": {
    "name": "BoBo",
    "email": "boboooooo159@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 10.16"
  },
  "scripts": {
    "start": "node deployment/guide.js",
    "dev": "vue-cli-service serve --mode dev",
    "build:out": "vue-cli-service build --mode out",
    "build:dev": "vue-cli-service build --mode developer",
    "pkg:out": "npm run build:out && node deployment/compression.js",
    "pkg:dev": "npm run build:dev && node deployment/compression.js",
    "deploy:intranet-public:out": "npm run build:out && node deployment/compression.js -INTRANETDEMO",
    "report:out": "vue-cli-service build --mode report",
    "lint": "npm run eslint",
    "log": "conventional-changelog --config ./node_modules/vue-cli-plugin-commitlint/lib/log -i CHANGELOG.md -s -r 0",
    "cz": "npm run log && git add . && git cz"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compressing": "^1.5.1",
    "core-js": "2.6.5",
    "crypto-js": "^4.0.0",
    "dayjs": "^1.8.28",
    "fastclick": "^1.0.6",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "vant": "^2.8.6",
    "vconsole": "^3.3.4",
    "vue": "2.6.11",
    "vue-class-component": "^7.2.5",
    "vue-navigation": "^1.1.4",
    "vue-property-decorator": "^9.0.0",
    "vue-router": "3.4.3",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-typescript": "~4.3.0",
    "@vue/cli-service": "^4.4.0",
    "@vue/eslint-config-airbnb": "^4.0.1",
    "@vue/eslint-config-typescript": "^5.0.2",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.0.2",
    "babel-plugin-jsx-v-model": "^2.0.3",
    "babel-plugin-lodash": "^3.3.4",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "compression-webpack-plugin": "^4.0.0",
    "eslint": "^6.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-vue": "^5.2.3",
    "fastclick": "^1.0.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "postcss-px-to-viewport": "^1.1.1",
    "prettier": "^1.19.1",
    "right-pad": "^1.0.1",
    "sass": "^1.26.10",
    "sass-loader": "^9.0.3",
    "typescript": "~3.8.3",
    "conventional-changelog-cli": "^2.1.0",
    "vue-cli-plugin-commitlint": "^1.0.12",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-template-compiler": "2.6.11",
    "webpack": "^4.35.3",
    "webpack-bundle-analyzer": "^3.3.2",
    "svg-sprite-loader": "^4.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,vue,ts}": [
      "npm run lint",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/vue-cli-plugin-commitlint/lib/cz"
    }
  }
}
